package list;

public class Main {
  public static void main(String[] args) {
	  LinkedList numbers = new LinkedList();
	  
	  numbers.addFirst(30);
	  numbers.addFirst(20);
	  numbers.addFirst(10);
	  numbers.addLast(40);
	  //System.out.println(numbers.node(0));
	  numbers.add(1, 15);
	  numbers.add(3, 25);
	  System.out.println(numbers.removeFirst());
	  
	  System.out.println(numbers);
	  
  }
}
